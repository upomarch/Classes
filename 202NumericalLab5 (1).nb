Cell[BoxData[
 RowBox[{
  RowBox[{"RiemannSum", "[", 
   RowBox[{"f_", ",", "left_", ",", "right_", ",", "\[CapitalDelta]x_"}], 
   "]"}], ":=", 
  RowBox[{
   RowBox[{"\[CapitalDelta]x", " ", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{"left", "+", 
        RowBox[{"k", " ", "\[CapitalDelta]x"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "0", ",", 
        RowBox[{
         RowBox[{"Floor", "[", 
          FractionBox[
           RowBox[{"(", 
            RowBox[{"right", "-", "left"}], ")"}], "\[CapitalDelta]x"], "]"}],
          "-", "1"}]}], "}"}]}], "]"}]}], "//", "N"}]}]], "Input",
 CellLabel->"In[15]:=",ExpressionUUID->"0835f06c-35ee-4ced-9059-18c00d5534a5"],

