Cell[BoxData[
 RowBox[{
  RowBox[{"IDFT", "[", 
   RowBox[{"a_", ",", "T_", ",", "k_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"[", "j", "]"}], "]"}], " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]", " ", "k", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"j", "-", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"Length", "[", "a", "]"}], "/", "2"}], "-", "1"}], 
             ")"}]}], ")"}], "/", 
          RowBox[{"Length", "[", "a", "]"}]}], ")"}]}]]}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", 
       RowBox[{"Length", "[", "a", "]"}]}], "}"}]}], "]"}], "//", 
   "N"}]}]], "Input",
 CellChangeTimes->{{3.859392985189451*^9, 3.8593931776282787`*^9}, {
