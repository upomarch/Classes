       RowBox[{
        RowBox[{"Length", "[", "list", "]"}], "<", 
        RowBox[{
         RowBox[{"Dimensions", "[", "M", "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"v", "=", 
         RowBox[{"pm1", "[", 
          RowBox[{"M", ",", "f", ",", "n", ",", " ", "list"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"list", ",", "v"}], "]"}], ";", 
        RowBox[{"(*", 
         RowBox[{
         "this", " ", "adds", " ", "each", " ", "value", " ", "to", " ", 
          "our", " ", "empty", " ", "list"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"f", "=", 
