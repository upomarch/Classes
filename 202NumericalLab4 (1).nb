   RowBox[{"M_", ",", "w_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "f", "}"}], ",", 
    RowBox[{
     RowBox[{"f", "=", "w"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"HermitianMatrixQ", "[", "M", "]"}], ",", ",", 
       RowBox[{"Print", "[", "\"\<Error\>\"", "]"}]}], "]"}], ";", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "continues", " ", "function", " ", "is", " ", "matrix", " ", "is", " ",
         "Hermitian"}], ",", " ", 
       RowBox[{"prints", " ", "\"\<error\>\"", " ", "if", " ", "not"}]}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"list", "=", 
